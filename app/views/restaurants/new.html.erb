
<div class="container">
  <div class="row">
    <div class="formulaire">

      <%= simple_form_for [@restaurant] do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :phone_number %>
        <%= f.input :category %>
        <%= f.submit "Add this restaurant", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
